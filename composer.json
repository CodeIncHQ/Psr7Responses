  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4",
    "codeinchq/lib-errordisplay": "^1.1"
  },
  "require-dev": {
